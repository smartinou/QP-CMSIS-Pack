    <release version="7.1.3" date="2022-11-18">
      View QP/C++ Revision History at: https://www.state-machine.com/qpcpp/history.html
    </release>
  </releases>

  <keywords>
    <!-- keywords for indexing -->
    <keyword>Quantum Leaps</keyword>
    <keyword>arm</keyword>
    <keyword>framework</keyword>
    <keyword>fsm</keyword>
