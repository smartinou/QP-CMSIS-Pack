    <keyword>samek</keyword>
    <keyword>QP</keyword>
    <keyword>QP/C</keyword>
    <keyword>QP/C++</keyword>
    <keyword>QP/Spy</keyword>
    <keyword>QP/QUTest</keyword>
    <keyword>real time</keyword>
    <keyword>embedded framework</keyword>
